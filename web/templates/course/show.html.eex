<h2>Show course</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @course.title %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @course.description %>
  </li>

  <li>
    <strong>Thumbnail url:</strong>
    <%= @course.thumbnail_url %>
  </li>

  <li>
    <strong>Hidden:</strong>
    <%= @course.hidden %>
  </li>

</ul>

<%= link "Edit", to: course_path(@conn, :edit, @course) %>
<%= link "Back", to: course_path(@conn, :index) %>
